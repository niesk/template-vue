{
  "name": "template-vue",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.11.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "gen-api": "node ./scripts/generate-api.mjs",
    "postinstall": "husky"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@vueuse/core": "^11.1.0",
    "ant-design-vue": "^4.2.5",
    "axios": "^1.5.1",
    "dayjs": "^1.11.10",
    "vue": "^3.5.12",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@cqfe/eslint-config": "^1.3.7",
    "@cqfe/generate-api": "0.11.8",
    "@cqfe/prettier-config": "^1.3.9",
    "@cqfe/stylelint-config": "^1.3.3",
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@types/node": "^18.0.2",
    "eslint": "^8.49.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.0.3",
    "sass": "^1.69.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.4.9",
    "husky": "^9.0.11"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prettier": "@cqfe/prettier-config",
  "eslintConfig": {
    "extends": "@cqfe/eslint-config"
  },
  "stylelint": {
    "extends": "@cqfe/stylelint-config"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier -c --write"
    ]
  }
